{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "linebreak-style": "off",
        "no-unused-vars": "off",
        "consistent-return": "off",
        "no-console":"off",
        "no-shadow": "off",
        "no-def": "off",
        "func-names": "off"
    }
}
